<?xml version="1.0" encoding="UTF-8"?>

<structure> 
    <command name = "Default">   
        <classroot>Logout</classroot>                                                       
    </command>
    <command name = "Login">  
        <status value = "CMD_OK">        
            <forward>LocationStructure</forward>              
        </status>
        <status value = "CMD_ERROR">        
            <view>LoginView</view>              
        </status>
        <status value = "CMD_INSUFFICIENT_DATA">        
           <view>LoginView</view>           
        </status>                                         
    </command>
    <command name = "Logout">   
        <view>LoginView</view>                                                       
    </command>
    <command name = "LocationStructure"> 
        <status value = "CMD_OK">    
            <view>LocationStructureView</view> 
        </status>                                                      
    </command> 
    <command name = "LocationStructureBack"> 
        <status value = "CMD_OK">    
            <view>LocationStructureView</view> 
        </status>                                                      
    </command> 
     
</structure> 

